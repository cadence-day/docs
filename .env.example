# Cadence.day Environment Variables
# Copy this file to .env.production for production or .env.development for local development

# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL="your_supabase_url_here"
EXPO_PUBLIC_SUPABASE_KEY="your_supabase_anon_key_here"

# Clerk Authentication
# Use pk_live_* for production, pk_test_* for development
EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY="your_clerk_publishable_key_here"

# Sentry 
EXPO_PUBLIC_SENTRY_DSN=""

# Sentry Configuration for Production Builds
SENTRY_AUTH_TOKEN=""
SENTRY_ORG=""
SENTRY_PROJECT=""

# Legacy Supabase Keys (to be removed after migration)
EXPO_PUBLIC_LEGACY_SUPABASE_URL=""
EXPO_PUBLIC_LEGACY_SUPABASE_KEY=""

# PostHog Key
EXPO_PUBLIC_POSTHOG_KEY=""
EXPO_PUBLIC_POSTHOG_HOST=""

# RevenueCat API Keys (add your actual keys here)
EXPO_PUBLIC_REVENUECAT_IOS_API_KEY="your_revenuecat_ios_api_key_here"
EXPO_PUBLIC_REVENUECAT_ANDROID_API_KEY="your_revenuecat_android_api_key_here"

# OpenAI API Key for Chat Feature (GPT-4o)
EXPO_PUBLIC_OPENAI_API_KEY="your_openai_api_key_here"

# Anthropic API Key (Legacy - can be removed)
EXPO_PUBLIC_ANTHROPIC_API_KEY="your_anthropic_api_key_here"